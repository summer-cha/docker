{
  "services": [
    {
      "name": "wyze-mjpeg-proxy",
      "image": "kamermans/wyze-mjpeg-proxy:latest",
      "isMain": true,
      "internalPort": 8190,
      "environment": {
        "RTSP_URL": "${RTSP_URL}",
        "RESOLUTION": "${RESOLUTION}",
        "QUALITY": "${QUALITY}",
        "FRAMERATE": "${FRAMERATE}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/config.yaml",
          "containerPath": "/config.yaml",
          "readOnly": true
        }
      ]
    }
  ]
}
